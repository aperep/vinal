{
  "name": "Sage",
  "image": "sagemath/sagemath:latest",
  "onCreateCommand": "sudo apt update && sudo apt install ca-certificates && sage --pip install tqdm",
  "customizations": {		
		"vscode": {
			"settings": { 
        "python.defaultInterpreterPath": "sage -python"
      },
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter",
        "Codeium.codeium"
      ]  
    }
  }
}
